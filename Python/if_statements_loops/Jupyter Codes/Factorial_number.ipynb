   "source": [
    "### Q:Write a code to calculate factorial of a number entered by user. "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter a positive digit number8\n",
      "the  factorial of 8 is 40320\n"
     ]
    }
   ],
   "source": [
    "fac = int(input(\"Enter a positive digit number\"))\n",
    "summ = 1\n",
    "\n",
    "if fac == 0 :\n",
    "    print(\"the factorial of number 0 is 1\")\n",
    "\n",
    "for i in range(1,fac+1):\n",
    "    summ *= i\n",
    "print(\"the  factorial of\", fac,\"is\", summ)"
   ]
  },
