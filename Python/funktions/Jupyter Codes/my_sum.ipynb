   "source": [
    "### Define a function named my_sum to return the sum of all int type inputted numbers."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "def my_sum(*a):\n",
    "    summ = 0\n",
    "    b = list(a)\n",
    "    for i in b:\n",
    "        summ += i\n",
    "    return summ"
   ]
  },
